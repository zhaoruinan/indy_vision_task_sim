<!-- Arm-Hand Fixed Joint -->
    <joint name="indy7_hand_joint" type="fixed">
        <parent link="indy7_link6" />
        <child link="indy7_hand" />
        <origin rpy="0 0 -0.7853981633974483" xyz="0 0 0.064" />
    </joint>


    <!-- Hand Links -->
    <link name="indy7_hand">
        <visual>
            <geometry>
                <mesh filename="package://indy7_ign/meshes/visual/hand.dae" />
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://indy7_ign/meshes/collision/hand.stl" />
            </geometry>
        </collision>
    </link>

    <link name="indy7_leftfinger">
        <visual>
            <geometry>
                <mesh filename="package://indy7_ign/meshes/visual/finger.dae" />
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://indy7_ign/meshes/collision/finger.stl" />
            </geometry>
        </collision>
    </link>

    <link name="indy7_rightfinger">
        <visual>
            <origin rpy="0 0 3.141592653589793" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://indy7_ign/meshes/visual/finger.dae" />
            </geometry>
        </visual>
