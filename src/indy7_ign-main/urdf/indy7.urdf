    <!-- Finger Joints -->
    <joint name="indy7_finger_joint1" type="prismatic">
        <parent link="indy7_hand" />
        <child link="indy7_leftfinger" />
        <origin rpy="0 0 0" xyz="0 0 0.0584" />
        <axis xyz="0 1 0" />
        <limit effort="20" lower="0.0" upper="0.04" velocity="0.2" />
    </joint>

    <joint name="indy7_finger_joint2" type="prismatic">
        <parent link="indy7_hand" />
        <child link="indy7_rightfinger" />
        <origin rpy="0 0 0" xyz="0 0 0.0584" />
        <axis xyz="0 -1 0" />
        <limit effort="20" lower="0.0" upper="0.04" velocity="0.2" />
        <mimic joint="indy7_finger_joint1" />
    </joint>


	<!-- End-effector -->
	<link name="indy7_link7"/>
	<joint name="end_effector_frame_fixed_joint" type="fixed">
		<!--<parent link="indy_hand"/>-->
